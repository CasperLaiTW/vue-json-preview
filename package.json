{
  "name": "vue-json-preview",
  "version": "0.0.6",
  "description": "debug json data in ease and fast",
  "homepage": "https://github.com/andriawan/vue-json-preview#readme",
  "keywords": [
    "vue",
    "json",
    "preview",
    "lib"
  ],
  "license": "MIT",
  "main": "dist/vue-json-preview.ssr.js",
  "browser": "dist/vue-json-preview.esm.js",
  "module": "dist/vue-json-preview.esm.js",
  "unpkg": "dist/vue-json-preview.min.js",
  "types": "vue-json-preview.d.ts",
  "files": [
    "dist/*",
    "vue-json-preview.d.ts",
    "src/**/*.vue"
  ],
  "author": {
    "name": "Muhammad Irwan Andriawan",
    "email": "irwan@andriawan.com"
  },
  "bugs": {
    "url": "https://github.com/andriawan/vue-json-preview/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andriawan/vue-json-preview.git"
  },
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-typescript": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "typescript": "^3.8.3",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "engines": {
    "node": ">=10"
  }
}
